<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#c0c0c0"
    android:orientation="vertical">
    <!--标题栏-->
    <com.mero.wyt_register.widget.CustomTitleBar
        android:id="@+id/title_bar_device_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text="设备信息"
        app:textSize="20sp"
        app:textColor="#FFF">
    </com.mero.wyt_register.widget.CustomTitleBar>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tx_sim_xulie_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="SIM卡序列号"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_sim_xulie_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_IMEI"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="IMEI"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_IMEI"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_IMSI"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="IMSI"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_IMSI"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="手机号码"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_phone_country"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="手机卡国家"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_phone_country"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_yunyingshang_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="运营商"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_yuunyingshang_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""/>
            <TextView
                android:id="@+id/tx_yunyingshang"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="运营商名字"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_yunyingshang"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_yunyingshang1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="运营商名字"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_yunyingshang2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_yunyingshang4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="运营商名字"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_yunyingshang22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_yunyingshang222"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="运营商名字"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_yunyingshang444"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
            <TextView
                android:id="@+id/tx_yunyingshang47"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="运营商名字"
                android:textColor="#000000"/>
            <EditText
                android:id="@+id/edt_yunyingshang7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:maxLines="1"/>
        </LinearLayout>

    </ScrollView>

</LinearLayout>
